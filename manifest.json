{
  "manifest_version": 3,
  "name": "Sidebar Bookmarks",
  "version": "1.0",
  "description": "Open bookmarked websites in the sidebar.",
  "permissions": ["sidePanel", "bookmarks"],
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "action": {
    "default_title": "Open Sidebar Bookmarks"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}